{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/server.ts"
    },
    {
      "src": "/public/uploads/(.*)",
      "dest": "public/uploads/$1"
    },
    {
      "src": "/(.*)",
      "dest": "src/server.ts"
    }
  ]
}
